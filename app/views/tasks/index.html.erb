<h1>Tasks#index</h1>
<p>Find me in app/views/tasks/index.html.erb</p>

<h2> Here is the lists of tasks : </h2>

<%= link_to "Create a task", new_task_path %>

<ul>
<% @tasks.each do |task| %>
<li>
  <p>
    Name : <%= task.name %>
  </p>
  <%= link_to "Show task", task_path(task) %>
</li>
<% end %>
</ul>
